{
  "name": "@pimpmyduck/assets",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "devDependencies": {
    "@svgr/parcel-plugin-svgr": "^5.3.0",
    "parcel-bundler": "^1.12.4",
    "svgo": "^1.3.2"
  },
  "scripts": {
    "build": "yarn svg:optimize && yarn svg:generate && yarn svg:build",
    "svg:optimize": "svgo --enable=prefixIds --disable=convertTransform -r -f ./src",
    "svg:generate": "node generate.js",
    "svg:build": "parcel build src/index.js --no-source-maps",
    "postinstall": "yarn build"
  }
}
