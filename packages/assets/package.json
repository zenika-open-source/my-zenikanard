{
  "name": "@pimpmyduck/assets",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "yarn assets:generate && rollup -c rollup.config.js",
    "assets:optimize": "svgo --enable=prefixIds --disable=convertTransform -r -f ./src",
    "assets:generate": "node generate.js",
    "postinstall": "yarn build"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^4.0.0",
    "@svgr/rollup": "^5.3.0",
    "rollup": "^2.3.1",
    "svgo": "^1.3.2",
    "typescript": "^3.8.3"
  }
}
